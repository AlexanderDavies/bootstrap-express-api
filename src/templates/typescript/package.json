{
    "name": "ts-template-project",
    "version": "1.0.0",
    "description": "Node express typescript template",
    "scripts": {
        "document": "typedoc --out docs src",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint ./src/**/*.ts --fix",
        "test": "jest --ci  --reporters=default --detectOpenHandles",
        "test:debug": "node --inspect-brk ../node_modules/.bin/jest --runInBand",
        "coverage": "jest --ci --collectCoverage --no-cache",
        "start:dev": "SWAGGER_UI=true tsnd ./src/server.ts",
        "start:debug": "tsnd --inspect=9229 src/server.ts",
        "start": "NODE_ENV=production SWAGGER_UI=true node dist/bundle.js",
        "build:prod": "webpack"
    },
    "author": "Alexander Davies",
    "license": "MIT",
    "engines": {
        "node": ">=12.13.0"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-openapi-validate": "^0.5.1",
        "express-rate-limit": "^5.2.6",
        "express-slow-down": "^1.4.0",
        "express-validator": "^6.10.0",
        "lodash": "^4.17.21",
        "openapi-express-router": "^1.0.5",
        "prettier": "^2.2.1",
        "swagger-routes-express": "^3.3.0",
        "swagger-ui-express": "^4.1.6",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@types/express-slow-down": "^1.3.0",
        "@types/jest": "^26.0.22",
        "@types/lodash": "^4.14.168",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "copy-webpack-plugin": "^8.1.1",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "husky": "^6.0.0",
        "jest": "26.6.3",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "supertest": "6.1.3",
        "ts-jest": "26.5.5",
        "ts-loader": "^9.1.0",
        "ts-node-dev": "^1.1.6",
        "typedoc": "^0.20.35",
        "typescript": "^4.2.4",
        "webpack": "^5.35.0",
        "webpack-cli": "^4.6.0"
    }
}
