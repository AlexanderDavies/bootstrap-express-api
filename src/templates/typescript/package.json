{
    "name": "ts-template-project",
    "version": "1.0.0",
    "description": "Node express typescript template",
    "scripts": {
        "document": "typedoc --out docs src",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint ./src/**/*.ts --fix",
        "test": "jest --ci  --reporters=default --detectOpenHandles",
        "test:debug": "node --inspect-brk ../node_modules/.bin/jest --runInBand",
        "coverage": "jest --ci --collectCoverage",
        "start:dev": "SWAGGER_UI=true tsnd ./src/server.ts",
        "start:debug": "tsnd --inspect=9229 src/server.ts",
        "start": "NODE_ENV=production SWAGGER_UI=true node dist/bundle.js",
        "build:prod": "webpack"
    },
    "author": "Alexander Davies",
    "license": "MIT",
    "engines": {
        "node": ">=12.13.0"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-openapi-validate": "^0.5.1",
        "express-rate-limit": "^5.1.3",
        "express-slow-down": "^1.3.1",
        "express-validator": "^6.6.1",
        "lodash": "^4.17.20",
        "openapi-express-router": "^1.0.3",
        "prettier": "^2.1.1",
        "swagger-routes-express": "^3.2.1",
        "swagger-ui-express": "^4.1.4",
        "uuid": "^8.3.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@types/express": "^4.17.8",
        "@types/express-rate-limit": "^5.1.0",
        "@types/express-slow-down": "^1.3.0",
        "@types/jest": "^26.0.14",
        "@types/lodash": "^4.14.161",
        "@types/supertest": "^2.0.10",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "copy-webpack-plugin": "^6.3.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "husky": "^4.3.0",
        "jest": "26.6.3",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "supertest": "6.0.1",
        "ts-jest": "26.4.3",
        "ts-loader": "^8.0.9",
        "ts-node-dev": "^1.0.0-pre.62",
        "typedoc": "^0.19.2",
        "typescript": "^4.0.3",
        "webpack": "^5.4.0",
        "webpack-cli": "^4.2.0"
    }
}
