{
  "name": "bootstrap-express-api",
  "version": "1.0.1",
  "description": "An opinionated NODE CLI for generating Javascript or Typescript Express apis",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderDavies/bootstrap-express-api"
  },
  "bin": {
    "bootstrap-express-api": "bin/bootstrap-express-api.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --ci  --reporters=default --detectOpenHandles",
    "prettify": "prettier --write 'src/**/*.js'",
    "lint": "eslint ./**/*.js --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "bootstrap-express-api",
    "cli",
    "typescript",
    "javascript",
    "api",
    "express",
    "jest",
    "prettier",
    "project",
    "template"
  ],
  "author": "Alexander Davies",
  "license": "ISC",
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "arg": "^5.0.0",
    "chalk": "^4.1.1",
    "execa": "^5.0.0",
    "inquirer": "^8.0.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "pkg-install": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  }
}
