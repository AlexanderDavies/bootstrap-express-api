  "author": "Alexander Davies",
  "license": "ISC",
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "inquirer": "^7.3.3",
    "listr": "^0.14.3",
    "lodash": "^4.17.20",
    "ncp": "^2.0.0",
    "pkg-install": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2"
