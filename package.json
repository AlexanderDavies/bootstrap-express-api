{
  "name": "create-node-api",
  "version": "1.0.0",
  "description": "An opinionated NODE CLI for generating Javascript or Typescript Express apis",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderDavies/create-node-cli"
  },
  "bin": {
    "create-node-api": "bin/create-node-api.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
